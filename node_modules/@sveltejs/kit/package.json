{
	"name": "@sveltejs/kit",
	"version": "1.0.0-next.114",
	"type": "module",
	"dependencies": {
		"@sveltejs/vite-plugin-svelte": "^1.0.0-next.10",
		"cheap-watch": "^1.0.3",
